{
  "manifest_version":2,
  "name":"YouTubeâ„¢ Rating Loader",
  "description":"Show the like/dislike ratio on video previews on YouTube.",
  "version":"1.1.0",
  "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "background"
  ],
  "background": {
    "scripts": ["thirdParty/jquery-2.1.3.min.js","background.js"]
  },
  "content_scripts": [
    {
      "run_at" : "document_end",
      "matches": ["https://www.youtube.com/*"],
      "js": ["thirdParty/jquery-2.1.3.min.js", "ratingCache.js", "getRating.js"],
      "css": ["getRating.css"]
    }
  ]
}
