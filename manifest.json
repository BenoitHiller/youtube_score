{
  "manifest_version":2,
  "name":"YouTubeâ„¢ Rating Loader",
  "description":"Show the like/dislike ratio on video previews on YouTube.",
  "version":"2.2.0",
  "icons": { 
    "16": "share/icon16.png",
    "48": "share/icon48.png",
    "128": "share/icon128.png"
  },
  "background": {
    "scripts": ["vendor/dist/jquery.js","background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "run_at" : "document_end",
      "matches": ["https://www.youtube.com/*"],
      "js": ["vendor/dist/jquery.js", "ratingCache.js", "observer.js"],
      "css": ["youtube.css"]
    }
  ]
}
